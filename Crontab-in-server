# Cronjob in Intermediate APP
# @hourly cp -r /home/bluestar/files/ /home/ubuntu/projects/rails_soap/public/ftp_upload/bluestar/ && sudo chown ubuntu:ubuntu /home/ubuntu/projects/rails_soap/public/ftp_upload/bluestar -R
# @hourly cp -r /home/venusftp/files/ /home/ubuntu/projects/rails_soap/public/ftp_upload/venusftp/ && sudo chown ubuntu:ubuntu /home/ubuntu/projects/rails_soap/public/ftp_upload/venusftp -R
# @hourly cp -r /home/ankitgems/files/ /home/ubuntu/projects/rails_soap/public/ftp_upload/ankitgems/ && sudo chown ubuntu:ubuntu /home/ubuntu/projects/rails_soap/public/ftp_upload/ankitgems -R
# @hourly rm -r /home/ubuntu/projects/rails_soap/public/ftp_upload/*

#
# Copying latest uploaded CSV file by BlueStar via FTP
@hourly cp $(ls -1t /home/bluestar/files/*.csv | head -n 1) /home/ubuntu/projects/rails_soap/public/ftp_upload/bluestar/ && sudo chown ubuntu:ubuntu /home/ubuntu/projects/rails_soap/public/ftp_upload/bluestar -R
#
